{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "arrow-parens": 0,
    "comma-dangle": 0,
    "import/no-named-as-default": 0,
    "max-len": ["error", 150],
    "no-mixed-operators": 0,
    "react/forbid-prop-types": 0,
    "react/static-property-placement": 0
  }
}